{
  "name": "farmaciaai-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:bootstrap": "node-sass .\\scss\\custom-bootstrap.scss .\\src\\assets\\css\\bootstrap.min.css",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles:prod && npm run compile:server",
    "build:ssr:prod": "npm run build:client-and-server-bundles:prod && npm run compile:server && npm run pack-dist",
    "build:client-and-server-bundles": "ng build && ng run farmaciaai-angular:server --bundleDependencies all",
    "build:client-and-server-bundles:prod": "ng build --prod && ng run farmaciaai-angular:server:production --bundleDependencies all",
    "pack-dist": "del dist.zip & @powershell -Command \"& 'C:\\Program Files\\7-Zip\\7z.exe' u dist.zip dist .ebextensions\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~20.0.1",

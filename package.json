    "@angular/platform-browser": "~20.0.1",
    "@angular/platform-browser-dynamic": "~20.0.1",
    "@angular/platform-server": "~20.0.1",
    "@angular/router": "~20.0.1",
    "@ng-bootstrap/ng-bootstrap": "^18.0.0",
    "@nguniversal/express-engine": "^16.2.0",
